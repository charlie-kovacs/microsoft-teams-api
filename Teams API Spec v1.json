    "paths": {
        "/v1.0/users": {
            "get": {
                "summary": "List Users",
                "description": "List information about users",
                "operationId": "listUsers",
                "responses": {
                    "200": {
                      "content": {
                        "application/json": {
                          "schema": {
                            "$ref": "#/components/schemas/Users"
                          }
                        }
                      },
                      "description": "SUCCESS"
                    }
                }
            }
        },
        "/v1.0/users/{userId}": {
            "get": {
                "summary": "List User",
                "description": "List information about a specific user",
                "operationId": "listUser",
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
